<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush Color="#3D5AFE" x:Key="InfoColor"/>
    <SolidColorBrush Color="#F44336" x:Key="ErrorColor"/>
    <SolidColorBrush Color="#FF8F00" x:Key="WarningColor"/>
    <SolidColorBrush Color="#4CAF50" x:Key="SuccessColor"/>

    <SolidColorBrush Color="#757575" x:Key="GrayTextColor"/>
    <SolidColorBrush Color="#CFD8DC" x:Key="MidGrayTextColor"/>
    <SolidColorBrush Color="#FAFAFA" x:Key="WhiteTextColor"/>
    <SolidColorBrush Color="#EEEEEE" x:Key="LightGrayColor"/>
    <SolidColorBrush Color="#263238" x:Key="DarkColorBg"/>
    <SolidColorBrush Color="#455A64" x:Key="LightDarkColorBg"/>

    <SolidColorBrush Color="{Binding Source={StaticResource MaterialDesignPaper}, Path=Color}" x:Key="MdPaperCustom"/>
</ResourceDictionary>