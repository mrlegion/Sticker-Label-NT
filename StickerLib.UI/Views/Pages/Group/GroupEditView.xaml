<Page x:Class="StickerLib.UI.Views.Pages.Group.GroupEditView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:StickerLib.UI.Views.Pages"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GroupEditView"
      DataContext="{Binding GroupEdit, Source={StaticResource Locator}}">

    <Grid>
        <Button VerticalAlignment="Center" 
                HorizontalAlignment="Center" 
                Content="Go to next page" 
                Command="{Binding NavigateToCommand}"
                CommandParameter="groupList"/>
    </Grid>
</Page>